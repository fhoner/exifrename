<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fhoner.exifrename.renameui.controller.RenameController">
    <children>
        <VBox prefHeight="120.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <children>
                <HBox>
                    <children>
                        <Label prefHeight="19.0" prefWidth="161.0" text="%sourceDir">
                            <padding>
                                <Insets top="3.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="txtSource" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </TextField>
                        <Button mnemonicParsing="false" onAction="#btnBrowseSource" text="...">
                            <HBox.margin>
                                <Insets left="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label prefHeight="19.0" prefWidth="161.0" text="%destinationDir">
                            <padding>
                                <Insets top="3.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="txtDestination" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </TextField>
                        <Button mnemonicParsing="false" onAction="#btnBrowseDestination" text="...">
                            <HBox.margin>
                                <Insets left="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label prefHeight="19.0" prefWidth="161.0" text="%pattern">
                            <padding>
                                <Insets top="3.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="txtPattern" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="26.0" prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%year">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%town">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </HBox>
                <HBox prefWidth="200.0">
                    <children>
                        <Pane prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefWidth="150.0" text="%month">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefWidth="150.0" text="%county">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="26.0" prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%day">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%state">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="26.0" prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%hour">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%country">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="26.0" prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%minute">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%countryCode">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="26.0" prefWidth="161.0" />
                        <Button mnemonicParsing="false" onAction="#insertVariable" prefHeight="26.0" prefWidth="150.0" text="%second">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                  <Label fx:id="lblVersion" text="\${version}">
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
                        <Pane HBox.hgrow="ALWAYS" />
                        <Label text="%progress">
                            <padding>
                                <Insets top="3.0" />
                            </padding>
                            <HBox.margin>
                                <Insets right="5.0" top="3.0" />
                            </HBox.margin>
                        </Label>
                  <Label fx:id="lblProgress" text="0/0">
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                     <HBox.margin>
                        <Insets right="20.0" top="3.0" />
                     </HBox.margin>
                  </Label>
                        <Button fx:id="btnMakeFiles" mnemonicParsing="false" onAction="#makeFiles" prefHeight="26.0" prefWidth="101.0" text="%go">
                            <HBox.margin>
                                <Insets bottom="2.0" right="4.0" top="2.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets top="25.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
